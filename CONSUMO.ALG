Algoritmo "consumo"

Var

    distancia, combustivel, consumo: real

Inicio

   escreva("Distância percorrida: ")
   leia(distancia)
   escreva("Litros de combustível: ")
   leia(combustivel)
   
   consumo <- distancia / combustivel
   
   escreva("O consumo médio do veículo foi de ", consumo:5:3, " Km/L.")

Fimalgoritmo